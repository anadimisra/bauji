<%= simple_form_for(@certification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :terms %>
    <%= f.input :seo_meta_keywords %>
    <%= f.input :seo_meta_description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
