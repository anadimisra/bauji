<%= simple_form_for(@certification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :city %>
    <%= f.input :venue %>
    <%= f.input :dates %>
    <%= f.input :schedule %>
    <%= f.input :seats %>
    <%= f.input :unit_price %>
    <%= f.input :venue_map_link %>
    <%= f.input :description %>
    <%= f.input :terms %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
